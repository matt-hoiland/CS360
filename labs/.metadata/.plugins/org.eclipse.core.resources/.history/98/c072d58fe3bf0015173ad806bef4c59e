#ifndef HTTPMESSAGE_H_
#define HTTPMESSAGE_H_

#include <map>
#include <string>
#include <vector>

using namespace std;

class HttpResponse {
private:
	string protocol;
	int responseCode;
	string responseMessage;

	map<string, vector<string> > headers;

};



#endif /* HTTPMESSAGE_H_ */
