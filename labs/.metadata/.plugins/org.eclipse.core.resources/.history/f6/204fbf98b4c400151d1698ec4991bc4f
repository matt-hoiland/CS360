#ifndef SERVER_H_
#define SERVER_H_

using namespace std;

class Server {
private:
	bool debug;
	unsigned int port;
	string dir;

	int hsocket;



public:
	Server(bool debug, unsigned int port, string dir) :
		debug(debug), port(port), dir(dir) {}
	~Server() {}

	void run() {
		cout << "Woot!" << endl;
	}
};



#endif /* SERVER_H_ */
